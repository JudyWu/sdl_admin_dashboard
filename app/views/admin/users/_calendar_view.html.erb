        <div class="fc-toolbar"> 
            <div class="fc-center">
                <button id="time_not_at_home_in_seconds_button" class="fc-button fc-state-default" type="button">Hours Away from Home</button>
                <button id="active_time_in_seconds_button" class="fc-button fc-state-default" type="button">Minutes Ambulatory</button>
                <button id="max_gait_speed_in_meter_per_second_button" class="fc-button fc-state-default"  type="button">Max Gait Speed</button>
                <button id="all_three_bars" class="fc-button fc-state-default"  type="button">View All</button>
            </div>          
        </div>

        <div class="container-fluid">
            <div>
                <div style="margin-bottom: 10px;">
                    <div id="calendar"></div>

                    <div id="filter_controls">
                        <span style="margin-right: 10px;"><input type="checkbox" id="show_bar_numbers" checked="checked" /> <label for="show_bar_numbers">display bar values</label></span>
                        <span><input type="checkbox" id="show_events" checked="checked" /> <label for="show_events">display events</label></span>
                    </div>
                </div>
                <div >
                    <div id="info">
                        <div id="info_content" style="display: none;">
                            <h2 style="margin: 0 0 10px 0;"></h2>

                            <table id="stats">
                                <tr>
                                    <td><span class="legend time_not_at_home_in_seconds"></span></td>
                                    <td class="stat_label">Hours Away from Home:</td>
                                    <td><span id="info_time_not_at_home_in_seconds" data-field="time_not_at_home_in_seconds" class="stat_value"></span></td>
                                </tr>
                                <tr>
                                    <td><span class="legend active_time_in_seconds"></span></td>
                                    <td class="stat_label">Minutes Ambulatory:</td>
                                    <td><span id="info_active_time_in_seconds" data-field="active_time_in_seconds" class="stat_value"></span></td>
                                </tr>
                                <tr>
                                    <td><span class="legend max_gait_speed_in_meter_per_second"></span></td>
                                    <td class="stat_label">Max Gait Speed:</td>
                                    <td><span id="info_max_gait_speed_in_meter_per_second" data-field="max_gait_speed_in_meter_per_second" class="stat_value"></span></td>
                                </tr>

                                <tr>
                                    <td style="">
                                        <div id="averaged-notice" style="color: #555; font-size: 13px;"> (averaged over the selected _s)</div>
                                    </td>
                                </tr>
                            </table>

                            <div id="info_notes">
                                <h3>Notes</h3>
                                <textarea style="width: 100%; height: 150px; resize: vertical;"></textarea>
                                <div style="text-align: right;">
                                    <button>Save Notes</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- /.container -->

